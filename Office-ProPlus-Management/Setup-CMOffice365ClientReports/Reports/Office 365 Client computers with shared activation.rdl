  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT dbo.System_DATA.Name0 AS ComputerName, 
SUBSTRING(dbo.Add_Remove_Programs_64_DATA.DisplayName00, 0 ,CHARINDEX('-',dbo.Add_Remove_Programs_64_DATA.DisplayName00)) AS ProgramName,
 SUBSTRING(dbo.Add_Remove_Programs_64_DATA.DisplayName00, CHARINDEX('-',dbo.Add_Remove_Programs_64_DATA.DisplayName00)+2 ,5) AS Language,
dbo.Add_Remove_Programs_64_DATA.Version00 AS Version,
dbo.Office365ProPlusConfigurations_DATA.Platform00 AS Bitness FROM dbo.Add_Remove_Programs_64_DATA
INNER JOIN dbo.System_DATA ON dbo.System_DATA.MachineID = dbo.Add_Remove_Programs_64_DATA.MachineID
INNER JOIN dbo.Office365ProPlusConfigurations_DATA ON dbo.Office365ProPlusConfigurations_DATA.MachineID = dbo.Add_Remove_Programs_64_DATA.MachineID
INNER JOIN v_FullCollectionMembership ON v_FullCollectionMembership.Name = dbo.System_DATA.Name0
WHERE [dbo].Office365ProPlusConfigurations_DATA.SharedComputerLicensing00 LIKE '1' AND dbo.Add_Remove_Programs_64_DATA.DisplayName00 LIKE 'Microsoft Office 365 ProPlus -%' AND dbo.Add_Remove_Programs_64_DATA.Version00 LIKE '16%' AND dbo.Add_Remove_Programs_64_DATA.DisplayName00 LIKE '%' + dbo.Office365ProPlusConfigurations_DATA.ClientCulture00 AND v_FullCollectionMembership.CollectionID = @CollectionID

UNION

