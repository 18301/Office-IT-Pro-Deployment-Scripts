  <Description>Displays all computers that have shared computer activation enabled.</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_">
