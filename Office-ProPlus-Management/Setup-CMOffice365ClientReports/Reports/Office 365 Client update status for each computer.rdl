    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>DECLARE @Status VARCHAR(100)
SET @Status = 'all'

SELECT sysd.Name0, 
substring(addr.DisplayName00, 0, charindex('-', addr.DisplayName00)) AS ProductName, 
custOf.ClientCulture00, 
custOf.VersionToReport00, 
cust.LastScenario00, 
cust.LastScenarioResult00,
UpdateSource = CASE 
WHEN (cust.GPOOfficeMgmtCOM00 IS NULL AND cust.OfficeMgmtCOM00 = 'True') THEN 'Configuration Manager' 
WHEN cust.GPOOfficeMgmtCOM00 = '1' THEN 'Configuration Manager' 
WHEN (cust.UpdateChannel00 IS NOT NULL AND cust.UpdateChannel00 != '') THEN 'Cloud'
WHEN cust.UpdatePath00 LIKE '\\%' THEN cust.UpdatePath00
WHEN cust.UpdatePath00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN cust.UpdateUrl00 LIKE '\\%' THEN cust.UpdateUrl00
WHEN cust.UpdateUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN cust.CDNBaseUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
END
FROM [dbo].Office365ProPlusConfigurations_DATA cust
JOIN dbo.Add_Remove_Programs_64_DATA  addr on addr.MachineID = cust.MachineID
INNER JOIN dbo.Office365ProPlusConfigurations_DATA custOf on cust.MachineID = custOf.MachineID
INNER JOIN dbo.System_DATA sysd ON sysd.MachineID = cust.MachineID
WHERE addr.DisplayName00 LIKE '%Microsoft Office 365 ProPlus -%'
AND addr.Version00 LIKE '16%'
AND addr.DisplayName00 like '%' + custOf.ClientCulture00 + '%'
AND cust.LastScenarioResult00 LIKE 
CASE WHEN @Status = 'all'
	 THEN '%'
	 ELSE @Status
END
UNION
SELECT sysd.Name0, 
substring(addr.DisplayName00, 0, charindex('-', addr.DisplayName00)) AS ProductName, 
custOf.ClientCulture00, 
custOf.VersionToReport00, 
cust.LastScenario00, 
cust.LastScenarioResult00,
UpdateSource = CASE 
WHEN (cust.GPOOfficeMgmtCOM00 IS NULL AND cust.OfficeMgmtCOM00 = 'True') THEN 'Configuration Manager' 
WHEN cust.GPOOfficeMgmtCOM00 = '1' THEN 'Configuration Manager' 
WHEN (cust.UpdateChannel00 IS NOT NULL AND cust.UpdateChannel00 != '') THEN 'Cloud'
WHEN cust.UpdatePath00 LIKE '\\%' THEN cust.UpdatePath00
WHEN cust.UpdatePath00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN cust.UpdateUrl00 LIKE '\\%' THEN cust.UpdateUrl00
WHEN cust.UpdateUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN cust.CDNBaseUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
END
FROM [dbo].Office365ProPlusConfigurations_DATA cust
JOIN dbo.Add_Remove_Programs_DATA  addr on addr.MachineID = cust.MachineID
INNER JOIN dbo.Office365ProPlusConfigurations_DATA custOf on cust.MachineID = custOf.MachineID
INNER JOIN dbo.System_DATA sysd ON sysd.MachineID = cust.MachineID
WHERE addr.DisplayName00 LIKE '%Microsoft Office 365 ProPlus -%'
AND addr.Version00 LIKE '16%'
AND addr.DisplayName00 like '%' + custOf.ClientCulture00 + '%'
AND cust.LastScenarioResult00 LIKE 
CASE WHEN @Status = 'all'
	 THEN '%'
	 ELSE @Status
END
UNION
SELECT sysd.Name0, 
substring(addr.DisplayName00, 0, charindex('-', addr.DisplayName00)) AS ProductName, 
custOf.ClientCulture00, 
custOf.VersionToReport00, 
cust.LastScenario00, 
cust.LastScenarioResult00,
UpdateSource = CASE 
WHEN cust.UpdatePath00 LIKE '\\%' THEN cust.UpdatePath00
WHEN cust.UpdatePath00 LIKE 'http%' THEN 'Cloud'
WHEN cust.UpdateUrl00 LIKE '\\%' THEN cust.UpdateUrl00
WHEN cust.UpdateUrl00 LIKE 'http%' THEN 'Cloud'
WHEN cust.CDNBaseUrl00 LIKE 'http%' THEN 'Cloud'
END
FROM [dbo].Office365ProPlusConfigurations2013_DATA cust
JOIN dbo.Add_Remove_Programs_64_DATA  addr on addr.MachineID = cust.MachineID
INNER JOIN dbo.Office365ProPlusConfigurations2013_DATA custOf on cust.MachineID = custOf.MachineID
INNER JOIN dbo.System_DATA sysd ON sysd.MachineID = cust.MachineID
WHERE addr.DisplayName00 LIKE '%Microsoft Office 365 ProPlus -%'
AND addr.Version00 LIKE '15%'
AND addr.DisplayName00 like '%' + custOf.ClientCulture00 + '%'
AND cust.LastScenarioResult00 LIKE 
CASE WHEN @Status = 'all'
	 THEN '%'
	 ELSE @Status
END
UNION
SELECT sysd.Name0, 
substring(addr.DisplayName00, 0, charindex('-', addr.DisplayName00)) AS ProductName, 
custOf.ClientCulture00, 
custOf.VersionToReport00, 
cust.LastScenario00, 
cust.LastScenarioResult00,
UpdateSource = CASE 
WHEN cust.UpdatePath00 LIKE '\\%' THEN cust.UpdatePath00
WHEN cust.UpdatePath00 LIKE 'http%' THEN 'Cloud'
WHEN cust.UpdateUrl00 LIKE '\\%' THEN cust.UpdateUrl00
WHEN cust.UpdateUrl00 LIKE 'http%' THEN 'Cloud'
WHEN cust.CDNBaseUrl00 LIKE 'http%' THEN 'Cloud'
END
FROM [dbo].Office365ProPlusConfigurations2013_DATA cust
JOIN dbo.Add_Remove_Programs_DATA  addr on addr.MachineID = cust.MachineID
INNER JOIN dbo.Office365ProPlusConfigurations2013_DATA custOf on cust.MachineID = custOf.MachineID
INNER JOIN dbo.System_DATA sysd ON sysd.MachineID = cust.MachineID
WHERE addr.DisplayName00 LIKE '%Microsoft Office 365 ProPlus -%'
AND addr.Version00 LIKE '15%'
AND addr.DisplayName00 like '%' + custOf.ClientCulture00 + '%'
AND cust.LastScenarioResult00 LIKE 
CASE WHEN @Status = 'all'
	 THEN '%'
	 ELSE @Status
END</CommandText>
