    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ComputerName">
            <Value>=Parameters!ComputerName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Channels">
            <Value>=Parameters!Channels.Value</Value>
          </QueryParameter>
