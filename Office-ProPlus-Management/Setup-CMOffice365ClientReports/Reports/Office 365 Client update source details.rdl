          </QueryParameter>
          <QueryParameter Name="@UpdatesEnabled">
            <Value>=Parameters!UpdatesEnabled.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ChannelTable TABLE(Channel VARCHAR(100) NULL, CPath VARCHAR(100));
INSERT INTO @ChannelTable VALUES ('Deferred','http://officecdn.microsoft.com/pr/7ffbc6bf-bc32-4f92-8982-f9dd17fd3114'),('FirstReleaseDeferred','http://officecdn.microsoft.com/pr/b8f9b850-328d-4355-9145-c59439a0c4cf'),('Current','http://officecdn.microsoft.com/pr/492350f6-3a01-4f97-b9c0-c7c6ddf67d60'),('FirstReleaseCurrent','http://officecdn.microsoft.com/pr/64256afe-f5d9-4f86-8936-8840a6a4f5be')
DECLARE @ResultsTable TABLE(ComputerName VARCHAR(100), ProductName VARCHAR(100), PrimaryLanguage VARCHAR(100), ProductVersion VARCHAR(100), Architecture VARCHAR(100), UpdatesEnabled VARCHAR(100), UpdateSource VARCHAR(100), Channel VARCHAR(100));
INSERT INTO @ResultsTable
SELECT
SysD.Name0,
SUBSTRING(Addr.DisplayName00, 0 ,CHARINDEX('-',Addr.DisplayName00)) AS ProgramName, 
OffCL.ClientCulture00,
OffCL.VersionToReport00,
OffCL.Platform00,
UpdateEnabled = CASE WHEN OffCL.UpdatesEnabled00 = 'True' THEN 'True' ELSE 'False' END,
UpdateSource = CASE 
WHEN (OffCL.GPOOfficeMgmtCOM00 IS NULL AND OffCL.OfficeMgmtCOM00 = 'True') THEN 'Configuration Manager' 
WHEN OffCL.GPOOfficeMgmtCOM00 = '1' THEN 'Configuration Manager' 
WHEN (OffCL.UpdateChannel00 IS NOT NULL AND OffCL.UpdateChannel00 != '') THEN 'Cloud'
WHEN OffCL.UpdatePath00 LIKE '\\%' THEN OffCL.UpdatePath00
WHEN OffCL.UpdatePath00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN OffCL.UpdateUrl00 LIKE '\\%' THEN OffCL.UpdateUrl00
WHEN OffCL.UpdateUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN OffCL.CDNBaseUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
END,
Channel = CASE 
WHEN (OffCL.GPOOfficeMgmtCOM00 IS NULL AND OffCL.OfficeMgmtCOM00 = 'True') THEN (SELECT Channel FROM @ChannelTable ct WHERE ct.CPath = OffCL.CDNBaseUrl00)
WHEN OffCL.GPOOfficeMgmtCOM00 = '1' THEN (SELECT Channel FROM @ChannelTable ct WHERE ct.CPath = OffCL.CDNBaseUrl00)
WHEN (OffCL.UpdateChannel00 IS NOT NULL AND OffCL.UpdateChannel00 != '') THEN OffCL.UpdateChannel00
WHEN OffCL.UpdatePath00 LIKE '\\%' THEN (SELECT Channel FROM @ChannelTable ct WHERE ct.CPath = OffCL.CDNBaseUrl00)
WHEN OffCL.UpdatePath00 LIKE 'http://officecdn%' THEN (SELECT Channel FROM @ChannelTable TCT WHERE TCT.CPath = OffCL.UpdatePath00)
WHEN OffCL.UpdateUrl00 LIKE '\\%' THEN (SELECT Channel FROM @ChannelTable ct WHERE ct.CPath = OffCL.CDNBaseUrl00)
WHEN OffCL.UpdateUrl00 LIKE 'http://officecdn%' THEN  (SELECT Channel FROM @ChannelTable TCT WHERE TCT.CPath = OffCL.UpdateUrl00)
WHEN OffCL.CDNBaseUrl00 LIKE 'http://officecdn%' THEN (SELECT Channel FROM @ChannelTable ct WHERE ct.CPath = OffCL.CDNBaseUrl00)
END
FROM
dbo.Add_Remove_Programs_64_DATA Addr
INNER JOIN dbo.Office365ProPlusConfigurations_DATA OffCL  ON OffCL.MachineID = Addr.MachineID
INNER JOIN dbo.System_DATA SysD ON SysD.MachineID = Addr.MachineID
WHERE Addr.DisplayName00 LIKE '%proplus%' AND Addr.Version00 LIKE '16%'


INSERT INTO @ResultsTable
SELECT
SysD.Name0,
SUBSTRING(Addr.DisplayName00, 0 ,CHARINDEX('-',Addr.DisplayName00)) AS ProgramName, 
OffCL.ClientCulture00,
OffCL.VersionToReport00,
OffCL.Platform00,
UpdateEnabled = CASE WHEN OffCL.UpdatesEnabled00 = 'True' THEN 'True' ELSE 'False' END,
UpdateSource = CASE 
WHEN (OffCL.GPOOfficeMgmtCOM00 IS NULL AND OffCL.OfficeMgmtCOM00 = 'True') THEN 'Configuration Manager' 
WHEN OffCL.GPOOfficeMgmtCOM00 = '1' THEN 'Configuration Manager' 
WHEN (OffCL.UpdateChannel00 IS NOT NULL AND OffCL.UpdateChannel00 != '') THEN 'Cloud'
WHEN OffCL.UpdatePath00 LIKE '\\%' THEN OffCL.UpdatePath00
WHEN OffCL.UpdatePath00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN OffCL.UpdateUrl00 LIKE '\\%' THEN OffCL.UpdateUrl00
WHEN OffCL.UpdateUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN OffCL.CDNBaseUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
END,
Channel = CASE 
WHEN (OffCL.GPOOfficeMgmtCOM00 IS NULL AND OffCL.OfficeMgmtCOM00 = 'True') THEN (SELECT Channel FROM @ChannelTable ct WHERE ct.CPath = OffCL.CDNBaseUrl00)
WHEN OffCL.GPOOfficeMgmtCOM00 = '1' THEN (SELECT Channel FROM @ChannelTable ct WHERE ct.CPath = OffCL.CDNBaseUrl00)
WHEN (OffCL.UpdateChannel00 IS NOT NULL AND OffCL.UpdateChannel00 != '') THEN OffCL.UpdateChannel00
WHEN OffCL.UpdatePath00 LIKE '\\%' THEN (SELECT Channel FROM @ChannelTable ct WHERE ct.CPath = OffCL.CDNBaseUrl00)
WHEN OffCL.UpdatePath00 LIKE 'http://officecdn%' THEN (SELECT Channel FROM @ChannelTable TCT WHERE TCT.CPath = OffCL.UpdatePath00)
WHEN OffCL.UpdateUrl00 LIKE '\\%' THEN (SELECT Channel FROM @ChannelTable ct WHERE ct.CPath = OffCL.CDNBaseUrl00)
WHEN OffCL.UpdateUrl00 LIKE 'http://officecdn%' THEN  (SELECT Channel FROM @ChannelTable TCT WHERE TCT.CPath = OffCL.UpdateUrl00)
WHEN OffCL.CDNBaseUrl00 LIKE 'http://officecdn%' THEN (SELECT Channel FROM @ChannelTable ct WHERE ct.CPath = OffCL.CDNBaseUrl00)
END
FROM
dbo.Add_Remove_Programs_DATA Addr
INNER JOIN dbo.Office365ProPlusConfigurations_DATA OffCL  ON OffCL.MachineID = Addr.MachineID
INNER JOIN dbo.System_DATA SysD ON SysD.MachineID = Addr.MachineID
WHERE Addr.DisplayName00 LIKE '%proplus%' AND Addr.Version00 LIKE '16%'

INSERT INTO @ResultsTable
SELECT
SysD.Name0,
SUBSTRING(Addr.DisplayName00, 0 ,CHARINDEX('-',Addr.DisplayName00)) AS ProgramName, 
OffCL.ClientCulture00,
OffCL.VersionToReport00,
OffCL.Platform00,
UpdateEnabled = CASE WHEN OffCL.UpdatesEnabled00 = 'True' THEN 'True' ELSE 'False' END,
UpdateSource = CASE 
WHEN OffCL.UpdatePath00 LIKE '\\%' THEN OffCL.UpdatePath00
WHEN OffCL.UpdatePath00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN OffCL.UpdateUrl00 LIKE '\\%' THEN OffCL.UpdateUrl00
WHEN OffCL.UpdateUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN OffCL.CDNBaseUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
END,
Channel = 'N/A'
FROM
dbo.Add_Remove_Programs_DATA Addr
INNER JOIN dbo.Office365ProPlusConfigurations2013_DATA OffCL  ON OffCL.MachineID = Addr.MachineID
INNER JOIN dbo.System_DATA SysD ON SysD.MachineID = Addr.MachineID
WHERE Addr.DisplayName00 LIKE '%proplus%' AND Addr.Version00 LIKE '15%'

INSERT INTO @ResultsTable
SELECT
SysD.Name0,
SUBSTRING(Addr.DisplayName00, 0 ,CHARINDEX('-',Addr.DisplayName00)) AS ProgramName, 
OffCL.ClientCulture00,
OffCL.VersionToReport00,
OffCL.Platform00,
UpdateEnabled = CASE WHEN OffCL.UpdatesEnabled00 = 'True' THEN 'True' ELSE 'False' END,
UpdateSource = CASE 
WHEN OffCL.UpdatePath00 LIKE '\\%' THEN OffCL.UpdatePath00
WHEN OffCL.UpdatePath00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN OffCL.UpdateUrl00 LIKE '\\%' THEN OffCL.UpdateUrl00
WHEN OffCL.UpdateUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
WHEN OffCL.CDNBaseUrl00 LIKE 'http://officecdn%' THEN 'Cloud'
END,
Channel = 'N/A'
FROM
dbo.Add_Remove_Programs_64_DATA Addr
INNER JOIN dbo.Office365ProPlusConfigurations2013_DATA OffCL  ON OffCL.MachineID = Addr.MachineID
INNER JOIN dbo.System_DATA SysD ON SysD.MachineID = Addr.MachineID
WHERE Addr.DisplayName00 LIKE '%proplus%' AND Addr.Version00 LIKE '15%'

UPDATE @ResultsTable
SET ComputerName = '' WHERE ComputerName IS NULL
UPDATE @ResultsTable
SET UpdatesEnabled = '' WHERE UpdatesEnabled IS NULL
UPDATE @ResultsTable
SET Channel = '' WHERE Channel IS NULL

 

SELECT * FROM @ResultsTable
WHERE ComputerName LIKE
CASE WHEN @ComputerName = 'All'
THEN '%'
ELSE @ComputerName
END
AND Channel LIKE
CASE WHEN @Channels = 'All'
THEN '%'
ELSE @Channels
END
AND UpdateSource LIKE
CASE WHEN @UpdateSource = 'All'
THEN '%'
WHEN @UpdateSource = 'Local'
THEN  '\\%'
ELSE @UpdateSource
END
AND UpdatesEnabled LIKE
CASE WHEN @UpdatesEnabled = 'All'
THEN '%'
ELSE @UpdatesEnabled
END</CommandText>
